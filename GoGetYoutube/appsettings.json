{
  "AppSettings": {
    "SaveLocationConfig": "-o /mnt/NAS/Youtube/%(uploader)s/%(title)s.%(ext)s --no-mtime",
    "QualityConfig": "-f bestvideo+bestaudio",
    "SubtitleConfig": "--write-srt --sub-lang en"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}